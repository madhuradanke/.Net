@using BOL;
@using System.Collections.Generic;
@{
    List<BOL.Product> products =( List <BOL.Product> )ViewData["catalog"];
     <h1>Hellooooo</h1>

         <ol>
             @{
             foreach(Product product in products)
                 {
                 <li>@product.pid
                 @product.Pname
                 @product.Qty
                @product.Price</li>
     }
     }
    </ol>
    
}

